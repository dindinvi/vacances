{% block body -%}
    <h1>Adresse list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Numerovoie</th>
                <th>Libelleadd</th>
                <th>Codepostal</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('adresse_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.numeroVoie }}</td>
                <td>{{ entity.libelleAdd }}</td>
                <td>{{ entity.codePostal }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('adresse_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('adresse_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('adresse_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
