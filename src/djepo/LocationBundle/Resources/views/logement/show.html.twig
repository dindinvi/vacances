{% extends '::base.html.twig' %}

{% block body -%}
    <h1>logement</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Nombrepiece</th>
                <td>{{ entity.nombrePiece }}</td>
            </tr>
            <tr>
                <th>Typeimmeuble</th>
                <td>{{ entity.typeImmeuble }}</td>
            </tr>
            <tr>
                <th>Surface</th>
                <td>{{ entity.surface }}</td>
            </tr>
            <tr>
                <th>Transport</th>
                <td>{{ entity.transport }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('logement') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('logement_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('logement_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
